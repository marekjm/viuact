; This is a oneline comment.

(let log (x) {
    (print x)
    0
})

(let logl (~x) {
    (print x)
    0
})

(let main () {
    (log "Hello, World!")

    (let y "Pun intended!")
    (logl (~x y))   ; bind different name

    (let x "What's up?")
    (print x)
    (logl (~x x))   ; bind the same name explicitly

    (let x "Same old.")
    (print x)
    (logl ~x)       ; use label punning

    (log "Good bye, World!")
    0
})
